
<br><br>

<h1 class="d-flex justify-content-center">Category Details</h1>

<br><br>

@model Category

@{
    Layout = "_App_Layout";
}

<table class="table">
  <tbody>
    <tr>
      <th>Category Id</th>
      <td>@Model.CategoryId</td>
    </tr>
    <tr>
      <th>Name</th>
      <td>@Model.Name</td>
    </tr>
    <tr>
      <th>Description</th>
      <td>@Model.Description</td>
    </tr>
  </tbody>
</table>
<br>
<a asp-controller="Category" asp-action="Index" class="btn btn-success">Back</a>